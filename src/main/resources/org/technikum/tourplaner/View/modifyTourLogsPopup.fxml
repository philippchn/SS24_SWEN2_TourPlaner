<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns:fx="http://javafx.com/fxml" fx:controller="org.technikum.tourplaner.controller.ModifyTourLogsPopupController" spacing="10" alignment="CENTER" prefWidth="400" prefHeight="300">
    <TextField fx:id="dateTextField" promptText="Date" />
    <TextField fx:id="commentTextField" promptText="Comment" />
    <TextField fx:id="difficultyTextField" promptText="Difficulty" />
    <TextField fx:id="totalDistanceTextField" promptText="Total Distance" />
    <TextField fx:id="totalTimeTextField" promptText="Total Time" />
    <TextField fx:id="ratingTextField" promptText="Rating" />
    <Button fx:id="saveButton" text="Save" />
</VBox>
